def find_N(m1, m2, c1, c2, e):
	# Find N using GCD(k1*N, k2*N)
	k1N = pow(m1, e) - c1
	k2N = pow(m2, e) - c2

	N = gcd(k1N,k2N)
	print 'gcd', N

	assert c1 == pow(m1, e, N)
	assert c2 == pow(m2, e, N)

	return N

